<%- include('cabecera', {titulo: 'Panel',usuario:usu}) -%>
<div class="section">
  <div class="container ">
    <br><br>
    <div class="row">
      <h1 class="header center-align">Panel de control</h1>
      <form action="admin" method="post" style="display: <%= admin_panel %>;">
        <input type="submit" value="Admin" class=" waves-effect waves-light blue lighten-1 btn orange">
      </form>
    <form class="col s12" method='post' action='password'>
      <div class="row">
        <div class="col s12">
          <h3 class="">Cambiar contraseña.</h3>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="password1" name='password1' type="password" class="validate">
          <label for="password1">Contraseña</label>
        </div>
      </div>

      <!-- A veces cuesta hacer click para entrar en este input-->
      
      <div class="row">
        <div class="input-field col s12">
          <input id="password2" name='password2' type="password" class="validate">
          <label for="password2">RepetirContraseña</label>
        </div>
      </div>
      <div class="row">
        <div class='input-field col s10'></div>
        <div class="input-field col s2">
          <input type="submit" value='Enviar' class="waves-effect waves-light blue lighten-1 btn orange">
        </div>
      </div>
      <hr>
    </form>
  </div>
</div>
<%- include('pie') -%>
