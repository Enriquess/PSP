<%- include('cabecera', {titulo: 'Admin',usuario:usu}) -%>
<div class="section">
    <div class="container ">
      <br><br>
      <div class="row">
        <h1 class="header center-align">Administracion</h1>
        <div class="row">
          <ul class="collection with-header">
            <li class="collection-header orange lighten-3"><h4>Usuarios</h4></li>
            <% 
              let f = null;
              for (let n = 0;n < filas.length ; n++){
                f = filas[n];
                %>
                <li class="collection-item orange lighten-4"><%= f.usuario %></li>
              <%}%>
        </div>
      <form class="col s12" method='post' action='alta'>
        <div class="row">
          <div class="col s12">
            <h3 class="">Dar alta.</h3>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="nombre" name='nombre' type="text" class="validate">
            <label for="nombre">Nombre</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="password" name='password' type="password" class="validate">
            <label for="password">Contraseña</label>
          </div>
        </div>
        <div class="row">
          <div class='input-field col s10'></div>
          <div class="input-field col s2">
            <input type="submit" value='Dar alta' class="waves-effect waves-light blue lighten-1 btn orange">
          </div>
        </div>
      </form>
      <form class="col s12" method='post' action='modificar'>
        <div class="row">
          <div class="col s12">
            <h3 class="">Modificar,</h3>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="nombre2" name='nombre2' type="text" class="validate">
            <label for="nombre2">Nombre</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="password2" name='password2' type="password" class="validate">
            <label for="password2">Contraseña</label>
          </div>
        </div>
        <div class="row">
          <div class='input-field col s10'></div>
          <div class="input-field col s2">
            <input type="submit" value='Modificar' class="waves-effect waves-light blue lighten-1 btn orange">
          </div>
        </div>
      </form>
      <form class="col s12" method='post' action='borrar'>
        <div class="row">
          <div class="col s12">
            <h3 class="">Borrar.</h3>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <input id="nombre3" name='nombre3' type="text" class="validate">
            <label for="nombre3">Nombre</label>
          </div>
        </div>
        <div class="row">
          <div class='input-field col s10'></div>
          <div class="input-field col s2">
            <input type="submit" value='Borrar' class="waves-effect waves-light blue lighten-1 btn orange">
          </div>
        </div>
      </form>
    </div>
  </div>
<%- include('pie') -%>